NODE_ENV=production
PORT=3001

# Database - será preenchido pelo Railway com variáveis de ambiente
# DB_HOST=
# DB_PORT=
# DB_USER=
# DB_PASSWORD=
# DB_NAME=

# CORS - Aceita Vercel e previews
# Exemplo: https://bpm-editor.vercel.app,https://*.vercel.app
CORS_ORIGIN=https://bpm-editor.vercel.app

# TypeORM - Em produção, desabilita synchronize
# Migration será executada via npm run start:prod
TYPEORM_SYNCHRONIZE=false
